<p id="notice"><%= notice %></p>

<h1>Listing Upload Urls</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Folder</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @upload_urls.each do |upload_url| %>
      <tr>
        <td><%= upload_url.code %></td>
        <td><%= upload_url.folder %></td>
        <td><%= link_to 'Show', upload_url %></td>
        <td><%= link_to 'Edit', edit_upload_url_path(upload_url) %></td>
        <td><%= link_to 'Destroy', upload_url, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Upload url', new_upload_url_path %>
